        <activity android:name=".Equipment.Equipment_AddElement"
            android:theme="@style/VaporsanaTheme"
            android:label="@string/app_name"
            >
        </activity>
        <activity android:name=".Kurvorschlag.RequestTreatment"
            android:theme="@style/VaporsanaTheme"
            android:label="@string/app_name"
            >
        </activity>
        <activity android:name=".Kurvorschlag.ProposeTreatment"
            android:theme="@style/VaporsanaTheme"
            android:label="@string/app_name"
            >
        </activity>
        <activity android:name=".Voting.Voting"
            android:theme="@style/VaporsanaTheme"
            android:label="@string/app_name"
            >
        </activity>
    </application>

</manifest>
